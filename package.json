{
    "name": "g4-engine-client",
    "displayName": "g4-engine-client",
    "description": "G4 Engine is a modern automation and RPA platform that unifies testing, data extraction, and workflow orchestration across technologies. Our tools empower developers and QA teams to build, monitor, and automate complex systems with precision and speed. We design seamless integrations, from desktop to cloud, to help teams accelerate delivery through intelligent automation.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.105.0"
    },
    "enabledApiProposals": [
        "treeViewActiveItem",
        "extensionRuntime"
    ],
    "publisher": "g4-api",
    "repository": {
        "type": "git",
        "url": "https://github.com/g4-api/g4-vscode-extension.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "license": "Apache-2.0",
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "New-Project",
                "category": "G4",
                "title": "Creates a New Rhino Project"
            },
            {
                "command": "Show-Workflow",
                "category": "G4",
                "title": "Open G4 Engine Embedded Application"
            },
            {
                "command": "Start-Automation",
                "category": "G4",
                "title": "Start Automation Workflow on G4 Engine"
            },
            {
                "command": "Update-Documents",
                "category": "G4",
                "title": "Fetch latest documents version & refresh documentation"
            },
            {
                "command": "Update-Environment",
                "category": "G4",
                "title": "Update or Create Environment Variables"
            },
            {
                "command": "Update-Template",
                "category": "G4",
                "title": "Update or Create Template Definitions"
            }
        ],
        "grammars": [
            {
                "language": "g4log",
                "scopeName": "source.g4.log",
                "path": "./languages/g4-log-tm-language.json"
            }
        ],
        "languages": [
            {
                "id": "g4log",
                "aliases": [
                    "G4Log",
                    "g4Log"
                ],
                "extensions": []
            }
        ],
        "notebooks": [
            {
                "id": "G4.MdJsonNotebook",
                "type": "G4.MdJsonNotebook",
                "displayName": "Markdown + JSON Notebook",
                "selector": [
                    {
                        "filenamePattern": "*.mdjson"
                    }
                ]
            }
        ],
        "views": {
            "g4Container": [
                {
                    "type": "webview",
                    "id": "g4View",
                    "name": "Engine Client",
                    "icon": "resources/g4-icon.svg"
                },
                {
                    "type": "webview",
                    "id": "g4RecorderView",
                    "name": "G4 Recorder",
                    "icon": "resources/g4-icon.svg"
                },
                {
                    "type": "tree",
                    "id": "g4Documentations",
                    "name": "G4 Documentations",
                    "icon": "resources/g4-documents-icon.svg"
                },
                {
                    "type": "tree",
                    "id": "g4BotsManager",
                    "name": "G4 Bots Manager",
                    "icon": "resources/g4-bots-icon.svg"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "g4Container",
                    "title": "G4",
                    "icon": "resources/g4-icon.svg"
                }
            ]
        }
    },
    "readme": "README.md",
    "licenses": [
        {
            "path": "LICENSE",
            "type": "Apache-2.0"
        }
    ],
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test",
        "rebuild:electron": "npx @electron/rebuild -f -w uiohook-napi -v 37.2.3"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.10",
        "@types/node": "20.x",
        "@types/vscode": "^1.102.0",
        "@typescript-eslint/eslint-plugin": "^8.31.1",
        "@typescript-eslint/parser": "^8.31.1",
        "@vscode/test-cli": "^0.0.11",
        "@vscode/test-electron": "^2.5.2",
        "eslint": "^9.25.1",
        "ts-loader": "^9.5.2",
        "typescript": "^5.8.3",
        "webpack": "^5.99.7",
        "webpack-cli": "^6.0.1"
    },
    "files": [
        "resources/**",
        "languages/**",
        "dist/**",
        "node_modules/**",
        "LICENSE",
        "README.md"
    ],
    "dependencies": {
        "@microsoft/signalr": "^8.0.7",
        "node-fetch": "^3.3.2",
        "uiohook-napi": "^1.5.4"
    }
}